{
  "compilerOptions": {
    "strict": true,
    "target": "es2017",
    "lib": ["es2017", "dom"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}
